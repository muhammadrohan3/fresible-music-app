<div class="-u-page-s">
  <div class="-u-page" style="margin: 0">
    <div class="submissions">
      <table class="table table-hover text-center submissions__table">
        <thead class="submissions__table--head">
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Stage Name</th>
            <th>Details</th>
          </tr>
        </thead>
        <tbody>
          <% pageData.forEach(({id, userId, firstName, lastName, stageName}) =>
          { %>
          <tr>
            <td>
              <a href="/fmadmincp/subscriber/<%= userId %>/artist/<%= id %>"
                ><%= id %></a
              >
            </td>
            <td>
              <%=firstName && lastName && firstName +' '+ lastName %>
            </td>
            <td><%=stageName %></td>
            <td>
              <a href="/fmadmincp/subscriber/<%= userId %>/artist/<%= id %>"
                >View</a
              >
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
