<section class="-u-page">
  <div
    id="analyticsInitiate"
    class="analyticsInitiate d-flex flex-column flex-md-row"
    data-stores="<%=JSON.stringify(pageData) %>"
  >
    <div class="analyticsInitiate__container mr-md-5">
      <div class="analyticsInitiate__header">
        <span>Streams</span>
        <button class="analyticsInitiate__header--btn" data-type="stream">
          +
        </button>
      </div>
      <div class="analyticsInitiate__body" id="analyticsInitiate-stream"></div>
    </div>
    <div class="analyticsInitiate__container mt-5 mt-md-0 ml-md-5">
      <div class="analyticsInitiate__header">
        <span>Downloads</span>
        <button class="analyticsInitiate__header--btn" data-type="download">
          +
        </button>
      </div>
      <div
        class="analyticsInitiate__body"
        id="analyticsInitiate-download"
      ></div>
    </div>
  </div>
  <div class="text-right -u-mw-450 mt-4">
    <button class="btn-primary py-2" id="analyticsInitiateSubmit">
      Inititate &rarr;
    </button>
  </div>
</section>
