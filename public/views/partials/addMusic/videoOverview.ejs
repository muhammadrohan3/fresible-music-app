<div class="overview__cat">
  <div class="overview__cat--name">
    <span
      class="iconify overview__cat--name-icon"
      data-icon="ion:folder-open-outline"
      data-inline="false"
    ></span>
    VIDEO INFORMATION
  </div>
  <ul class="overview__cat__list">
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Title:</span>
      </div>
      <span><%= title && title %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Artiste:</span>
      </div>
      <span><%= artiste && artiste %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Featured:</span>
      </div>
      <span><%= featured && (featured || 'none') %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Genre:</span>
      </div>
      <span><%= genre && genre %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Track:</span>
      </div>
      <div class="d-inline-block">
        <span class="mr-1"><%= track ? 'link submitted' : '' %></span>
        <% if(Fns.isAdminPage) { %>
        <a target="_blank" href="https://<%= track %>"
          ><span
            class="iconify"
            data-icon="feather:eye"
            data-inline="false"
          ></span
        ></a>
        <% } %>
      </div>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Artwork:</span>
      </div>
      <div class="d-inline-block">
        <span class="mr-1"><%= artwork ? 'uploaded' : '' %></span>
        <% if(Fns.isAdminPage) { %>
        <a target="_blank" href="<%= artwork %>"
          ><span
            class="iconify"
            data-icon="feather:eye"
            data-inline="false"
          ></span
        ></a>
        <% } %>
      </div>
    </li>
  </ul>
</div>
