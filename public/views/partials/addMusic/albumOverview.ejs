<div class="overview__cat">
  <div class="overview__cat--name">
    <span
      class="iconify overview__cat--name-icon"
      data-icon="ion:folder-open-outline"
      data-inline="false"
    ></span>
    ALBUM INFORMATION
  </div>
  <ul class="overview__cat__list">
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Title:</span>
      </div>
      <span><%= name && name %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Tracks:</span>
      </div>
      <span><%= tracks && tracks.length %></span>
    </li>
    <li class="overview__cat__list__item">
      <div class="u-flex-middle">
        <span
          class="iconify overview__cat__list__item--icon"
          data-icon="ei:check"
          data-inline="false"
        ></span>
        <span>Artwork:</span>
      </div>
      <span><%= artwork ? 'uploaded' : '' %></span>
    </li>
  </ul>
  <% if(!Fns.pageUrl.includes('add-music')) { %>
  <div class="text-center">
    <a
      href="<%= Fns.url(`/submission/album?id=${Fns.idLookUp(id)}`) %>"
      class="btn-outline"
      >View Album &rarr;</a
    >
  </div>
  <% } %>
</div>
