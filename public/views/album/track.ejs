<div class="-u-page submission">
  <!-- <div><a href="/submissions" class="-u-link-default">&larr;</a></div> -->
  <% const {id, name, artwork, tracks} = pageData; %> <% const {title,
  id:trackId, artiste, featured, genre, explicit, copyrightHolder,
  copyrightYear, track } = tracks[0]; %>
  <div class="single-head">
    <a
      class="single-head__back"
      href="<%= Fns.url(`/submission/album?id=${Fns.idLookUp(id)}`) %>"
    >
      <span
        class="iconify"
        data-icon="ion:arrow-back-circle-outline"
        data-inline="false"
      ></span>
      <span>Back</span>
    </a>
    <div class="single-head__title -u-id-head">
      <span>Album Track</span>
      <span>&mdash;</span>
      <span><%= Fns.idLookUp(trackId) %></span>
    </div>
  </div>

  <div class="submission__body">
    <div class="overview__cat">
      <div class="overview__cat--name">
        <span
          class="iconify overview__cat--name-icon"
          data-icon="ion:folder-open-outline"
          data-inline="false"
        ></span>
        ALBUM INFO.
      </div>
      <ul class="overview__cat__list">
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Title:</span>
          </div>
          <span><%= name %></span>
        </li>
      </ul>
      <div class="text-center">
        <a
          href="<%= Fns.url(`/submission/album?id=${Fns.idLookUp(id)}`) %>"
          class="btn-outline"
          >&larr; Back to Album</a
        >
      </div>
    </div>
    <div class="overview__cat">
      <div class="overview__cat--name">
        <span
          class="iconify overview__cat--name-icon"
          data-icon="ion:folder-open-outline"
          data-inline="false"
        ></span>
        TRACK INFORMATION
      </div>
      <ul class="overview__cat__list">
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Title:</span>
          </div>
          <span><%= title%></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Artiste:</span>
          </div>
          <span><%= artiste %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Featured:</span>
          </div>
          <span><%= featured %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Genre:</span>
          </div>
          <span><%= genre %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Explicit Content:</span>
          </div>
          <span><%= explicit %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Copyright Holder:</span>
          </div>
          <span><%= copyrightHolder %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Copyright Year:</span>
          </div>
          <span><%= copyrightYear %></span>
        </li>
        <li class="overview__cat__list__item">
          <div class="u-flex-middle">
            <span
              class="iconify overview__cat__list__item--icon"
              data-icon="ei:check"
              data-inline="false"
            ></span>
            <span>Track:</span>
          </div>
          <div class="d-inline-block">
            <span class="mr-1"><%= track ? 'uploaded' : '' %></span> <%
            if(Fns.isAdminPage) { %>
            <a target="_blank" href="<%= track %>"
              ><span
                class="iconify"
                data-icon="feather:eye"
                data-inline="false"
              ></span
            ></a>
            <% } %>
          </div>
        </li>
      </ul>
    </div>
  </div>
</div>
