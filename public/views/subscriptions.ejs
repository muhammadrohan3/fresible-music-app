<div class="-u-page-s">
  <div class="-u-page-s__icon">
    <a href="/select-package"
      ><button class="btn-outline icon-btn">
        <span
          class="iconify"
          data-icon="ic:outline-add"
          data-inline="false"
        ></span
        ><span>New Subscription</span>
      </button></a
    >
  </div>
  <div class="-u-page" style="margin: 0;">
    <div class="submissions">
      <table class="table table-hover text-center submissions__table">
        <thead class="submissions__table--head">
          <tr>
            <th>Id</th>
            <th>Package</th>
            <th>Status</th>
            <th>Added</th>
            <th>Period</th>
          </tr>
        </thead>
        <tbody>
          <% pageData.forEach(({id, status, updatedAt, package, releases}) => {
          %>
          <tr>
            <td>
              <a href="/subscription/<%=Fns.idLookUp(id) %>"
                ><%=Fns.idLookUp(id) %></a
              >
            </td>
            <td><%=package.package %></td>
            <td>
              <span class="badge badge-pill badge-<%= Fns.colorText(status) %>"
                ><%=status %></span
              >
            </td>
            <td><%= releases.length %></td>
            <td>1 year</td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
